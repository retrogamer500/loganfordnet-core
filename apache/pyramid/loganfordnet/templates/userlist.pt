<div metal:use-macro="load: include/layout.pt">
    <div metal:fill-slot="content">
        <h2>Users</h2>
        
        <table>
            <tr>
                <th>Username</th>
                <th>User ID</th>
                <th>Created date</th>
            </tr>
            <tr tal:repeat="user users">
                <td><a href="/user/${user.id}/">${user.name}</a></td>
                <td>${user.id}</td>
                <td>${user.created_date}</td>
            </tr>
        </table>
    </div>
</div>
